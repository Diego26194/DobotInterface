<launch> 
<!-- Properties -->
 <node pkg="rosserial_python" type="serial_node.py" name="ARDUINO_SERVOS" output="screen">
    <param name="port" value="/dev/ttyACM1"/>
    <param name="baud" value="115200"/>
  </node>

 
  <node pkg="rosserial_python" type="serial_node.py" name="ARDUINO_MOTPAP" output="screen">
    <param name="port" value="/dev/ttyACM0"/>
    <param name="baud" value="115200"/>
  </node>  

</launch>
